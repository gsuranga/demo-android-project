<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid
                    android:color="#214C60"/>
            <stroke
                    android:width="1dp"
                    android:color="#214C60"/>
            <corners
                    android:radius="16dp"/>
            <padding
                    android:left="10dp"
                    android:top="10dp"
                    android:right="10dp"
                    android:bottom="10dp"/>
            
           
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                    android:startColor="#4297C0"
                    android:centerColor="#4297C0"
                    android:endColor="#4297C0"
                    android:angle="270"/>
            <stroke
                    android:width="1dp"
                    android:color="#4297C0"/>
            <corners
                    android:radius="16dp"/>
            <padding
                    android:left="10dp"
                    android:top="10dp"
                    android:right="10dp"
                    android:bottom="10dp"/>
        </shape>
    </item>
</selector>